version: '3'
services:
  static-maps:
    image: nginx:1.15-alpine
    # ports:
    #   - "8001:80"
    volumes:
      - ./nginx:/etc/nginx/conf.d
      - ./data:/var/www/maps
    restart: unless-stopped
    logging:
      driver: "journald"
      options:
        tag: "{{.Name}}"
      
networks:
  default:
    name: trufi-server
